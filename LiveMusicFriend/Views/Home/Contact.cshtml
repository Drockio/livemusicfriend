@{
    ViewBag.Title = "Contact Derek";
}

@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h2>@ViewBag.Message</h2>
                <h1>@ViewBag.Title </h1>
            </hgroup>
            <section class="contact">
                <header>
                    <h3>Email</h3>
                </header>
                <p>
                    <span class="label">Questions or Comments:</span>
                    <span><a href="mailto:Derek.Dunnom@Gmail.com">Derek.Dunnom@Gmail.com</a></span>
                </p>
            </section>
        </div>
    </section>
}


